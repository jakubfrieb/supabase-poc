{
  "name": "supabase-poc",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "start:tunnel": "expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android:apk": "npx expo prebuild --platform android --clean && cd android && ./gradlew assembleRelease && cd ..",
    "build:android:debug": "npx expo prebuild --platform android --clean && cd android && ./gradlew assembleDebug && cd ..",
    "build:android:standalone": "npx expo export --platform android && npx expo prebuild --platform android --clean && cd android && ./gradlew assembleRelease && cd ..",
    "install:android": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "install:android:debug": "adb install -r android/app/build/outputs/apk/debug/app-debug.apk",
    "install:eas": "adb install -r $(ls -t build-*.apk | head -1)"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.5.0",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/native-stack": "^7.6.2",
    "@supabase/supabase-js": "^2.80.0",
    "expo": "~54.0.23",
    "expo-auth-session": "^7.0.8",
    "expo-camera": "~17.0.9",
    "expo-constants": "^18.0.10",
    "expo-crypto": "^15.0.7",
    "expo-dev-client": "~6.0.17",
    "expo-document-picker": "^14.0.7",
    "expo-font": "~14.0.9",
    "expo-image-picker": "^17.0.8",
    "expo-localization": "^17.0.7",
    "expo-notifications": "~0.29.0",
    "expo-print": "^15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "^15.0.9",
    "i18next": "^25.6.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.2.4",
    "react-native": "0.81.5",
    "react-native-image-viewing": "^0.2.2",
    "react-native-qrcode-svg": "^6.3.20",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@expo/metro-config": "^54.0.9",
    "@types/react": "~19.1.0",
    "dotenv": "^17.2.3",
    "expo-build-properties": "^1.0.9",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "react-is": "18.2.0",
    "assert": "2.1.0"
  },
  "private": true
}
